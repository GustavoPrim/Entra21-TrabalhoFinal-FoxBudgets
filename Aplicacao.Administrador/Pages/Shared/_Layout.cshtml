                                </span>
                            </a>
                        </li>
                        <li class="nav-item nav-item-submenu">
                            <a href="#" class="nav-link"><i class="icon-copy"></i> <span>Administrador</span></a>

                            <ul class="nav nav-group-sub" data-submenu-title="Layouts">
                                <li class="nav-item"><a asp-controller="Administrador" asp-action="Index" class="nav-link">Listar Administradores</a></li>
                                <li class="nav-item"><a  asp-controller="Administrador" asp-action="Cadastrar"  class="nav-link">Cadastrar administrador</a></li>
                            </ul>

                            <ul class="nav nav-group-sub" data-submenu-title="Layouts">
                                <li class="nav-item"><a asp-controller="Administrador" asp-action="ListarFornecedor" class="nav-link">Listar Fornecedores</a></li>
                                <li class="nav-item"><a  asp-controller="Administrador" asp-action="CadastrarFornecedor"  class="nav-link">Cadastrar fornecedor</a></li>
                                <li class="nav-item"><a  asp-controller="Administrador" asp-action="Sair"  class="nav-link">Sair da conta</a></li>
                            </ul>
                        </li>
                        <!-- /layout -->

                    </ul>
                </div>
                <!-- /main navigation -->

            </div>
            <!-- /sidebar content -->

        </div>
        <!-- /main sidebar -->
        <!-- Main content -->
        <div class="content-wrapper">

            <!-- Content area -->
            <div class="content">

                @RenderBody()

            </div>
            <!-- /content area -->

        </div>
        <!-- /main content -->

    </div>
