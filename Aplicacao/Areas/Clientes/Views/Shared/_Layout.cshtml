                                    <span class="d-block font-weight-normal opacity-50"></span>
                                </span>
                            </a>
                        </li>
                        <li class="nav-item nav-item-submenu">
                            <a href="#" class="nav-link"><i class="icon-copy"></i> <span>Orçamentos</span></a>
                            <ul class="nav nav-group-sub" data-submenu-title="Layouts">
                                <li class="nav-item">
                                    <a asp-controller="Cotacao" asp-action="cotar"
                                       class="nav-link">Cotar</a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="Orcamento" asp-action="Realizados"
                                       class="nav-link">Realizados</a>
                                </li>
                            </ul>
                        </li>
                        <!-- /layout -->

                    </ul>
                </div>
                <!-- /main navigation -->

            </div>
            <!-- /sidebar content -->

        </div>
        <!-- /main sidebar -->
        <!-- Main content -->
        <div class="content-wrapper">

