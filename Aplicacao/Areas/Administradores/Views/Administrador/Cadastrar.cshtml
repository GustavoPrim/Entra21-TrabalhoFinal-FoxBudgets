@model AdministradorCadastrarViewModel

@{
    var administradores = ViewBag.Administradores as List<string>;
    administradores.Add(string.Empty);

    var selectDosFornecedores = new SelectList(administradores, string.Empty);
}
<p class="h3">Cadastro de Administradores</p>
<form asp-action="cadastrar" method="POST">

    <div class="mb-3">
        <label asp-for="Nome" class="form-label" span-required=""></label>
        <input asp-for="Nome" class="form-control" />
        <span asp-validation-for="Nome" class="text-danger"></span>
    </div>

    <div class="mb-3">
        <label asp-for="Cpf" class="form-label" span-required=""></label>
        <input asp-for="Cpf" class="form-control" />
        <span asp-validation-for="Cpf" class="text-danger"></span>
    </div>

    <div class="mb-3">
        <label asp-for="DataNascimento" class="form-label" span-required=""></label>
        <input asp-for="DataNascimento" class="form-control" />
        <span asp-validation-for="DataNascimento" class="text-danger"></span>
    </div>

    <div class="mb-3">
        <label asp-for="Endereco" class="form-label" span-required=""></label>
        <input asp-for="Endereco" class="form-control" />
        <span asp-validation-for="Endereco" class="text-danger"></span>
    </div>

    <div class="mb-3">
        <label asp-for="Email" class="form-label" span-required=""></label>
        <input asp-for="Email" class="form-control" />
        <span asp-validation-for="Email" class="text-danger"></span>
    </div>

    <div class="mb-3">
        <label asp-for="Telefone" class="form-label" span-required=""></label>
        <input asp-for="Telefone" class="form-control" />
        <span asp-validation-for="Telefone" class="text-danger"></span>
    </div>

    <div class="mb-3">
        <label asp-for="Login" class="form-label" span-required=""></label>
        <input type="text" asp-for="Login" class="form-control" id="login" />
        <span asp-validation-for="Login" class="text-danger"></span>
    </div>

    <div class="mb-3">
        <label asp-for="Senha" class="form-label" span-required=""></label>
        <input type="password" asp-for="Senha" class="form-control" id="senha" />
        <span asp-validation-for="Senha" class="text-danger"></span>
    </div>

    <div class="text-end">
        <a href="/administrador" class="btn btn-outline-danger">Cancelar</a>
        <button type="submit" class="btn btn-success">Salvar</button>
    </div>

</form>